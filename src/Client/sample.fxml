<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:controller="Client.Controller" xmlns:fx="http://javafx.com/fxml" alignment="center">
    <TextArea fx:id="textArea" editable="false" VBox.vgrow="ALWAYS"/>
    <HBox>
        <TextField fx:id="textField" HBox.hgrow="ALWAYS" promptText="Введите текст..." onAction="#sendMsg" />
        <Button fx:id="btn1" text="Send message" onAction="#sendMsg" />
    </HBox>
</VBox>

